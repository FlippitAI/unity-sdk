{
  "name": "com.flippit.flippitstudio",
  "version": "1.0.0",
  "displayName": "Flippit Studio",
  "description": "This plugin allows you to quickly and easily convert any NPC Character to Smart NPC with ai personality",
  "unity": "2021.3",
  "dependencies": {
    "com.unity.cinemachine": "2.8.9",
    "com.unity.textmeshpro": "3.0.6"
  },
  "keywords": [
    "openai",
    "chatgpt",
    "ai",
    "whisper",
    "speech",
    "vocal",
    "websocket"
  ],
  "author": {
    "Name": "Anthony Magdelaine",
    "email": "Tony@blackantmaster.com",
    "url": "https://blackantmaster.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FlippitAI/unity-sdk.git"
  },
  "scopedRegistries": [
    {
      "name": "GltFast",
      "url": "https://github.com/atteneder/glTFast.git",
      "scopes": [
        "com.atteneder.gltfast"
      ]
    },
    {
      "name": "NativeWebSocket",
      "url": "https://github.com/endel/NativeWebSocket.git",
      "scopes": [
        "com.endel.nativewebsocket"
      ]
    },
    {
      "name": "RpmCore",
      "url": "https://github.com/readyplayerme/rpm-unity-sdk-core.git",
      "scopes": [
        "com.readyplayerme.core"
      ]
    },
    {
      "name": "RpmWebView",
      "url": "https://github.com/readyplayerme/rpm-unity-sdk-webview.git",
      "scopes": [
        "com.readyplayerme.webview"
      ]
    },
    {
      "name": "OpenAI",
      "url": "https://github.com/srcnalt/OpenAI-Unity.git",
      "scopes": [
        "com.srcnalt.openai-unity"
      ]
    }
  ]
}
